{
  "manifest_version": 3,
  "name": "coding challenge chrome extension",
  "description": "coding challenge chrome extension",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "tab/newtab.html"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "popup/hello_extension.png"
  },
  "permissions": ["storage"]
}
